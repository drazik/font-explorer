{
  "name": "font-explorer",
  "productName": "Font Explorer",
  "version": "1.0.0",
  "description": "An application to explore the fonts installed on your system",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "babel src --out-dir dist"
  },
  "repository": "https://github.com/drazik/font-explorer",
  "keywords": [
    "font",
    "explorer"
  ],
  "author": "Cyrille Perois <cperois@outlook.com>",
  "license": "CC0-1.0",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "electron": "~1.6.2",
    "electron-packager": "^8.7.2"
  },
  "dependencies": {
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "styled-components": "^2.1.1",
    "system-font-families": "^0.5.0"
  },
  "babel": {
    "presets": [
      "react"
    ],
    "plugins": [
      "transform-class-properties"
    ]
  }
}
